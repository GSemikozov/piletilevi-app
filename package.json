{
  "name": "piletilevi-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.client.json && vite build --base=./",
    "prettier": "prettier \"**/**/*{.js,jsx,ts,tsx}\" --write --ignore-unknown",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "NODE_ENV=test jest",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.8",
    "@mui/material": "^5.14.5",
    "@mui/styled-engine-sc": "^5.12.0",
    "@reduxjs/toolkit": "^1.9.5",
    "gh-pages": "^6.0.0",
    "i18next": "^23.4.4",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.1.2",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-swipeable-views": "npm:@gromy/react-swipeable-views@^0.15.1",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^6.0.7",
    "tss-react": "^4.9.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@swc/core": "^1.3.78",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-swipeable-views": "^0.13.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "camelcase": "6",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^14.0.0",
    "msw": "^1.2.3",
    "prettier": "3.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "lint-staged": {
    "src/**": [
      "yarn prettier",
      "yarn lint"
    ]
  },
  "homepage": "https://gsemikozov.github.io/piletilevi-test-app/"
}
